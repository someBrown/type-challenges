{
    "compilerOptions": {
      "module": "esnext",
      "target": "es2017",
      "lib": ["ESNext","dom"],
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "allowJs": true,
      "exactOptionalPropertyTypes":true
    },
    "exclude": ["node_modules", "dist"]
  }
  